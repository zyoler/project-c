int validasipass(char a[]){
	int valhur=0;int huruf2=0;
    if(a[0]==' '||a[strlen(a)-1]==' '){
   		huruf2=1;
      	return huruf2;
    }
	for(int x=0;x<strlen(a);x++){
 		if(!(a[x]>='a'&&a[x]<='z'||a[x]>='A'&&a[x]<='Z'||a[x]>='0'&&a[x]<='9')){
    		valhur++;
 		}
	}
   return valhur;
}

