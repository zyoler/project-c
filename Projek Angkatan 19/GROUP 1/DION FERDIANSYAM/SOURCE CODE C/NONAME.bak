   void asciiSquare(int p,int l,int x,int y,int c){
   int ascii;
   textcolor(c);
	for(int a=1;a<=p+2;a++){
   	gotoxy(x-1+a,y);
      	if(a==1||a==p+2){
         	if(a==1)
            	ascii=201;
            else
            	ascii=187;
         }
         else{
         	ascii=205;
         }
         cprintf("%c",ascii);
   }
   for(int a=1;a<=p+2;a++){
   	gotoxy(x-1+a,y+l+1);
      	if(a==1||a==p+2){
         	if(a==1)
            	ascii=200;
            else
            	ascii=188;
         	}
         	else{
         		ascii=205;
         	}
         	cprintf("%c",ascii);
   	}
   	for(int a=1;a<=l;a++){
   		gotoxy(x,y+a);cprintf("%c",186);
      	gotoxy(x+p+1,y+a);cprintf("%c",186);
   	}
	}
   void csquare(int p,int l,int x,int y,int c){
      textcolor(c);
		for(int a=0;a<=p+1;a++){
   		gotoxy(x+a,y);cprintf("%c",219);
      	gotoxy(x+a,y+l+1);cprintf("%c",219);
   	}
   	for(int a=0;a<=l+1;a++){
   		gotoxy(x,y+a);cprintf("%c",219);
      	gotoxy(x+1,y+a);cprintf("%c",219);
      	gotoxy(x+p+3,y+a);cprintf("%c",219);
      	gotoxy(x+p+2,y+a);cprintf("%c",219);
   	}
	}
   void cline(int p,int l,int x,int y,int c){
      textcolor(c);
   	for(int a=0;a<l;a++){
      	for(int b=0;b<p;b++){
         	gotoxy(x+b,y+a);cprintf("%c",219);
         }
      }
   }


