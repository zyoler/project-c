   int indeksPenjual,indeksProduk,banyak;
   while(a<=banyakMembeli){
   	indeksPenjual=indeksPenjualTerbeli[a];
      indeksProduk=indeksProductTerbeli[a];
      banyak=indeksProductHarga[a];
      sellersaldo[indeksPenjual]+=OstokHarga[indeksProduk][1]*banyak;
      OstokHarga[indeksProduk][0]-=banyak;
   	a++;
   }
   saldo[indeksAkun]-=tHarga;
   updateDataProduk();
   saldoSeller(false);
   updateSaldo();
   banyakMembeli=0;
   //transaksiBerhasil
   loginBuyer();
}
