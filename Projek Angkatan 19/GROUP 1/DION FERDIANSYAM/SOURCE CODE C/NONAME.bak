   		if(inp[i]==64&&inp[i+1]==103&&inp[i+2]==109&&inp[i+3]==97&&inp[i+4]==105&&inp[i+5]==108&&inp[i+6]==46&&inp[i+7]==99&&inp[i+8]==111&&inp[i+9]==109){
      		return true;
      	}
      return false;
	}
