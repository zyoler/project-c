int numberValidation(char inp[],int max){
	int longChar,bad=0;
   	longChar=strlen(inp)-1;
      	for(int x=0;x<=longChar;x++){
         	if(!(inp[x]<=57&&inp[x]>=48)){
            	bad++;
            }

         }
      if(bad!=0||longChar<max-1){
      	return false;
      }
      else{
      	return true;
      }
}
int rupiahValidation(char inp[],int min,int max){
	int longChar,bad=0;
   	longChar=strlen(inp)-1;
      	for(int x=0;x<=longChar;x++){
         	if(!(inp[x]<=57&&inp[x]>=48)){
            	bad++;
            }
         }
    if(bad==0){
    	int a=atoi(inp);
      	if(a>=min && a<=max){
         	return true;
         }
    }
    return false;
}

int alphabetValidation(char inp[],int min,int place){
	int longChar,bad=0,luck=0;
      longChar=strlen(inp)-1;
      	for(int x=0;x<=longChar;x++){
         	if(inp[x]<=90&&inp[x]>=65||inp[x]<=122&&inp[x]>=97||inp[x]==32||inp[x]==39||inp[x]==45){
            	if(!(inp[x]==inp[x-1]&&inp[x-2]==inp[x-3])){
            		if(inp[x]==32){
               		if(x==0||x==longChar||inp[x]==inp[x-1]){
                  		bad++;
                  	}
               	}
               	else if(inp[x]==39||inp[x]==45){
               		if(x==0||x<1||inp[x-1]==inp[x]||x==longChar){
                  		bad++;
                  	}
                  	luck++;
            		}
               }
               else{
               	bad++;
               }
            }
            else{
            	bad++;
            }
         }
      if(bad==0&&longChar>=min-1&&luck<=1&&place!=0){
      	return true;
      }
      else if(bad==0&&longChar>=min-1&&luck<=0&&place==0){
      	return true;
      }
      else{
      	return false;
      }

}
int smallValidation(char inp[],int min,int email){
	int longChar,bad=0,luck=0,ok=0,slash=0;
   int titik=0,gmail=0;
      longChar=strlen(inp)-1;
      	for(int x=0;x<=longChar;x++){
         	if(inp[x]<=90&&inp[x]>=65||inp[x]<=122&&inp[x]>=97||inp[x]<=57&&inp[x]>=48){
            	if((inp[x]<=90&&inp[x]>=65)||(inp[x]<=57&&inp[x]>=48)){
               	if(inp[x]<=57){
                  	ok++;
                  }
                  else{
               		luck++;
                  }
               }
            }
            else if(inp[x]==95&&email==1){
            	slash++;
            }
            else if(email==1&&inp[x]==64||email==1&&inp[x]==46){
               if(inp[x]==64){
               	titik++;
               }
               else{
               	gmail++;
               }
            }
            else{
            	bad++;
            }
         }


      if(bad==0&&longChar>=min-1&&luck<=1&&ok<=10&&email==0){
      	return true;
      }
      else if(bad==0&&longChar>=min-1&&luck<=1&&ok<=10&&slash<=1&&email==1&&gmail==1&&titik<3){
         return true;
      }
      else{
      	return false;
      }

}

int dateValidation(char h[],char b[],char t[]){
   char copya[10],copyb[10],copyc[10];
   int hari,bulan,tahun;
   if(numberValidation(h,2)!=true||numberValidation(b,2)!=true||numberValidation(t,4)!=true){
   	return false;
   }
   strcpy(copya,h);strcpy(copyb,b);strcpy(copyc,t);
   hari=atoi(copya);
   bulan=atoi(copyb);
   tahun=atoi(copyc);
	if(hari<=31&&hari>=1&&bulan>=1&&bulan<=12&&tahun>=1945&&tahun<=2005){
   if(tahun%4==0){
   	if(bulan<=7&&bulan%2==1||bulan>=8&&bulan%2==0){
      	if(hari<=31){
         	return true;
         }
         else{
         	return false;
         }
      }
      else{
      	if(bulan==2 && hari<=29){
         	return true;
         }
         else{
         	if(bulan!=2 && hari<=30){
            	return true;
            }
            else{
            	return false;
            }
         }
      }
   }
   else{
   	if(bulan<=7&&bulan%2==1||bulan>=8&&bulan%2==0){
      	if(hari<=31){
         	return true;
         }
         else{
         	return false;
         }
      }
      else{
      	if(bulan==2 && hari<=28){
         	return true;
         }
         else{
         	if(bulan!=2 && hari<=30){
            	return true;
            }
            else{
            	return false;
            }
         }
      }
   }
  }
  else{
  	return false;
  }
}
	int phoneValidation(char inp[],int min){
         if(numberValidation(inp,min)==false){
         	return false;
         }
      	if(inp[0] == 48 && inp[1] == 56){
         	if(inp[2]!=52 && inp[2]!=54){
            	if(inp[2]==49||inp[2]==53&&inp[3]!=52||inp[2]==56){
               	return true;
               }
               else if(inp[2]==50||inp[2]==51){
               	if(inp[3]<=51){
                  	return true;
                  }
               }
               else if(inp[2]==55&&inp[3]<=56&&inp[3]>=55){
               	return true;
               }
               else if(inp[2]==57&&inp[3]>=53&&inp[3]<=57){
               	return true;
               }
            }
         }
         return false;
}
