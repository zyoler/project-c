bool valnohp(char nohp[]){

	if(!(nohp[0]=='0' && nohp[1]=='8')){
   	return false;
   }

   if(!(strlen(nohp)>=11 && strlen(nohp)<=13)){
   	return false;
   }

   for (int a=0;a<strlen(nohp);a++){
   	if(!(nohp[a]>='0' && nohp[a]<='9')){
   		return false;
      }
   }

   return true;
}
