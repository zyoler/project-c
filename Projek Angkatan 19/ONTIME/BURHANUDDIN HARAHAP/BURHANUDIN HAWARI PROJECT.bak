bool valnama(char nama[]){
	if(nama[0]==' ' ||  nama[strlen(nama)-1]==' '){
     		return false;
     }
     if(nama[0]==NULL){
         return false;
     }
     for(int a=0; nama[a]!=NULL; a++){
   		if(!(nama[a]>='a' && nama[a]<='z' || nama[a]>='A' && nama[a]<='Z' || nama[a]==' ')){
      		return false;
      	}
         if(nama[a]==' ' && nama[a-1]==' '){
            return false;
         }
      }
      return true;
}
