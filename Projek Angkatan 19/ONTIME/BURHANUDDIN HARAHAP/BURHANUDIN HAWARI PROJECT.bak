bool valumur(char umur[100]){

	if(umur[0]=='0' && umur[1]=='0' || umur[0]=='0' ){
   	return false;
   }
   for(int i=0; i<strlen(umur); i++){
   	if(!(umur[i]>='0' && umur[i]<='9')){
   		return false;
   	}
   }
   if(!(strlen(umur)<=2)){
   	return false;
   }
   else{
   	return true;
   }
}
