bool val_uang_men(char uang_men[]){
int cek=0;
	if(uang_men==NULL || uang_men[0] == '0'){
   	cek++;
   }
	for(int j=0; j<strlen(uang_men); j++){
   	if(!(uang_men[j] >= '0' && uang_men[j] <= '9')){
      	cek++;
      }
   }
   if(cek > 0){
   	return false;
   }else{
   	return true;
   }
}


