bool val_uang_women(char uang_women[]){
int cek=0;
	if(uang_women==NULL || uang_women[0] == '0'){
   	cek++;
   }
	for(int j=0; j<strlen(uang_women); j++){
   	if(!(uang_women[j] >= '0' && uang_women[j] <= '9')){
      	cek++;
      }
   }
   if(cek > 0){
   	return false;
   }else{
   	return true;
   }
}


