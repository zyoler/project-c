bool val_jumlah(char jumlah[]){
int cek=0;
	if(jumlah==NULL || jumlah[0] == '0'){
   	cek++;
   }
	for(int j=0; j<strlen(jumlah); j++){
   	if(!(jumlah[j] >= '0' && jumlah[j] <= '9')){
      	cek++;
      }
   }
   if(cek > 0){
   	return false;
   }else{
   	return true;
   }
}


