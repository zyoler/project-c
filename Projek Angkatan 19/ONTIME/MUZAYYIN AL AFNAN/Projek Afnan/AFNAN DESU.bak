bool val_uang_aksesoris(char uang_aksesoris[]){
int cek=0;
	if(uang_aksesoris==NULL || uang_aksesoris[0] == '0'){
   	cek++;
   }
	for(int j=0; j<strlen(uang_aksesoris); j++){
   	if(!(uang_aksesoris[j] >= '0' && uang_aksesoris[j] <= '9')){
      	cek++;
      }
   }
   if(cek > 0){
   	return false;
   }else{
   	return true;
   }
}



