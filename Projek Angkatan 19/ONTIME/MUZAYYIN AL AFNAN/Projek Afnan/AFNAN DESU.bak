bool val_uang_premium(char uang_premium[]){
int cek=0;
	if(uang_premium==NULL || uang_premium[0] == '0'){
   	cek++;
   }
	for(int j=0; j<strlen(uang_premium); j++){
   	if(!(uang_premium[j] >= '0' && uang_premium[j] <= '9')){
      	cek++;
      }
   }
   if(cek > 0){
   	return false;
   }else{
   	return true;
   }
}



