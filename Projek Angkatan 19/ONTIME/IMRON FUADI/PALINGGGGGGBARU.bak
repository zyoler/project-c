int validasihuruf(char a[]){
	int valhur=0; int huruf2=0;
   	if(a[0]==' '||a[strlen(a)-1]==' '||a[0]=='\''){
      	huruf2=1;
      	return huruf2;
    	}
	for(int i=0;i<strlen(a);i++){
 		if(!(a[i]>='a'&&a[i]<='z'||a[i]>='A'&&a[i]<='Z'||a[i]==' '&&a[i+1]!=' '||a[i]=='\''&&a[i+1]!='\''||a[i]=='-'&&a[i+1]!='-'||a[i]==' '&&a[i+1]!='-'&&a[i+1]!=' ')){
    		valhur++;
  		}
	}
   return valhur;
}

